{
  "name": "remix-app-template",
  "type": "module",
  "esnext": "node",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "workspaces": [
    "common/components",
    "common/design",
    "common/api"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "npm run update:seed && npm run build:new-styles && npm run build:styles && cross-env NODE_ENV=production node -r dotenv/config | remix vite:build --sourcemap",
    "dev": "npm run update:seed && npm run build:styles:watch | npm run build:new-styles:watch | cross-env NODE_ENV=development node -r dotenv/config | remix vite:dev",
    "start": "cross-env NODE_ENV=production node -r dotenv/config remix-serve ./build/server/index.js",
    "create": "plop",
    "format": "lint-staged",
    "build:styles": "npx tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/legacy-styles.css --minify",
    "build:new-styles": "npm run styles::build -w design",
    "build:styles:watch": "npm run build:styles -- --watch",
    "build:new-styles:watch": "npm run styles::develop -w design",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "npm run build:styles:watch | start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prisma:migrate": "node -r dotenv/config node_modules/.bin/prisma migrate dev",
    "prisma:format": "prisma format",
    "copy:fonts": "rsync node_modules/@fontsource/source-sans-pro/files/source-sans-pro-latin-{400,600,700}-normal.woff public/fonts",
    "make": "make",
    "update:seed": "npx tsx scripts/update-seed.ts",
    "typecheck": "npx tsc",
    "typecheck:api": "npx tsc -p ./common/api",
    "start:api": "ts-node ./common/api/src/server.ts"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.1",
    "@conform-to/react-v1": "npm:@conform-to/react@^1.0.2",
    "@conform-to/zod": "^0.9.1",
    "@conform-to/zod-v1": "npm:@conform-to/zod@^1.0.2",
    "@fontsource/source-sans-pro": "^5.0.8",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.7.1",
    "@remix-run/node": "^2.15.0",
