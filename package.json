{
  "name": "remix-app-template",
  "type": "module",
  "esnext": "node",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "workspaces": [
    "common/components",
    "common/design",
    "common/api"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "npm run build:new-styles && npm run build:styles && react-router build",
    "build:release": "cross-env TRIGGER_SENTRY_RELEASE=\"true\" npm run build",
    "dev": "npm run build:styles:watch | npm run build:new-styles:watch | react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "create": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=plopfile.ts",
    "format": "lint-staged",
    "build:styles": "npx tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/legacy-styles.css --minify",
    "build:new-styles": "npm run styles::build -w design",
    "build:styles:watch": "npm run build:styles -- --watch",
    "build:new-styles:watch": "npm run styles::develop -w design",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prisma:migrate": "node -r dotenv/config node_modules/.bin/prisma migrate dev",
    "prisma:format": "prisma format",
    "copy:fonts": "rsync node_modules/@fontsource/source-sans-pro/files/source-sans-pro-latin-{400,600,700,900}-normal.woff public/fonts",
    "make": "make",
    "typecheck": "react-router typegen && tsc",
    "typecheck:api": "npx tsc -p ./common/api",
    "typegen": "react-router typegen",
    "start:api": "ts-node ./common/api/src/server.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@conform-to/react": "^0.9.1",
    "@conform-to/react-v1": "npm:@conform-to/react@^1.2.2",
    "@conform-to/zod": "^0.9.1",
    "@conform-to/zod-v1": "npm:@conform-to/zod@^1.2.2",
    "@fontsource/source-sans-pro": "^5.0.8",
    "@hookform/resolvers": "^3.3.2",
    "@lexical/react": "^0.23.1",
    "@mjackson/file-storage": "^0.6.1",
    "@mjackson/form-data-parser": "^0.7.0",
    "@prisma/client": "^5.7.1",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@sentry/node": "^9.2.0",
    "@sentry/profiling-node": "^9.2.0",
    "@sentry/react": "^9.2.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.46.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@typescript-eslint/parser": "^8.24.0",
    "classnames": "^2.3.2",
    "commander": "^11.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "domain-functions": "^2.5.1",
    "dotenv": "^16.3.1",
    "file-type": "^20.1.0",
    "fs-extra": "^11.2.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "handlebars": "^4.7.8",
    "ics": "^3.7.1",
    "inquirer": "^9.2.12",
    "isbot": "^5.1.17",
    "js-cookie": "^3.0.5",
    "jsonschema": "^1.4.1",
    "jszip": "^3.10.1",
    "lexical": "^0.23.1",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "pica": "^9.0.1",
    "postcss": "^8.4.49",
    "rc-slider": "^10.5.0",
    "react": "^19.0.0",
    "react-countup": "^6.5.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.49.2",
    "react-image-crop": "^11.0.1",
    "react-router": "^7.1.5",
    "remix-forms": "^2.2.1",
    "remix-utils": "^8.1.0",
    "sanitize-html": "^2.11.0",
    "simple-git": "^3.21.0",
    "tailwindcss-opentype": "^1.1.0",
    "ts-node": "^10.9.2",
    "yup": "^1.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.3.1",
    "@mint-vernetzt/components": "workspace:*",
    "@react-router/dev": "^7.1.5",
    "@sentry/vite-plugin": "^3.1.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pica": "^9.0.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sanitize-html": "^2.9.5",
    "@types/ws": "^8.5.10",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.2",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "daisyui": "^4.4.20",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "mailhog": "^4.16.0",
    "plop": "^4.0.1",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.2",
    "prisma": "^5.7.1",
    "supabase": "^1.226.4",
    "tailwindcss": "^3.4.13",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.14",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.2",
    "vitest-mock-extended": "^2.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  }
}
