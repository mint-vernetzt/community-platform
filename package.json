{
  "name": "remix-app-template",
  "type": "module",
  "esnext": "node",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "workspaces": [
    "common/components",
    "common/design",
    "common/api"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "npm run build:new-styles && npm run build:styles && cross-env NODE_ENV=production node -r dotenv/config node_modules/.bin/remix build",
    "dev": "npm run build:styles:watch | npm run build:new-styles:watch | cross-env NODE_ENV=development node -r dotenv/config node_modules/.bin/remix dev",
    "start": "cross-env NODE_ENV=production node -r dotenv/config node_modules/.bin/remix-serve build",
    "create": "plop",
    "format": "lint-staged",
    "build:styles": "tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/legacy-styles.css --postcss",
    "build:new-styles": "npm run styles::build -w design",
    "build:styles:watch": "npm run build:styles -- --watch",
    "build:new-styles:watch": "npm run styles::develop -w design",
    "test": "cross-env NODE_ENV=test node -r dotenv/config node_modules/.bin/jest",
    "test:watch": "npm run test -- --watch",
    "storybook": "npm run build:styles:watch | start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prisma:migrate": "node -r dotenv/config node_modules/.bin/prisma migrate dev",
    "prisma:format": "prisma format",
    "copy:fonts": "rsync node_modules/@fontsource/source-sans-pro/files/source-sans-pro-latin-{400,600,700}-normal.woff public/fonts",
    "make": "make",
    "typecheck": "npx tsc",
    "typecheck:api": "npx tsc -p ./common/api"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.1",
    "@conform-to/zod": "^0.9.1",
    "@fontsource/source-sans-pro": "^5.0.8",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.7.1",
    "@remix-run/node": "2.4.0",
    "@remix-run/react": "2.4.0",
    "@remix-run/serve": "2.4.0",
    "@remix-run/server-runtime": "2.4.0",
    "@remix-run/web-fetch": "^4.4.2",
    "@supabase/auth-helpers-remix": "^0.2.6",
    "@supabase/supabase-js": "^2.39.1",
    "classnames": "^2.3.2",
    "commander": "^11.1.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "domain-functions": "^2.5.1",
    "dotenv": "^16.3.1",
    "file-type": "^18.7.0",
    "fs-extra": "^11.2.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "handlebars": "^4.7.8",
    "ics": "^3.7.1",
    "imgproxy": "^1.2.0",
    "inquirer": "^9.2.12",
    "jsonschema": "^1.4.1",
    "jszip": "^3.10.1",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "pica": "^9.0.1",
    "postcss": "^8.4.32",
    "rc-slider": "^10.5.0",
    "react": "^18.2.0",
    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-image-crop": "^11.0.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.21.0",
    "remix-auth": "^3.6.0",
    "remix-auth-supabase": "^4.0.2",
    "remix-domains": "^0.2.0",
    "remix-forms": "^2.2.1",
    "remix-utils": "^7.3.0",
    "sanitize-html": "^2.11.0",
    "simple-git": "^3.21.0",
    "tailwindcss-opentype": "^1.1.0",
    "ts-node": "^10.9.2",
    "yup": "^1.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.3.1",
    "@mint-vernetzt/components": "workspace:*",
    "@remix-run/dev": "2.4.0",
    "@remix-run/eslint-config": "2.4.0",
    "@remix-run/v1-route-convention": "^0.1.4",
    "@storybook/addon-actions": "^7.6.5",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^7.6.5",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.6.5",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pica": "^9.0.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/sanitize-html": "^2.9.5",
    "@types/ws": "^8.5.10",
    "@types/yup": "^0.29.14",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "daisyui": "^4.4.20",
    "depcheck": "^1.4.7",
    "esbuild": "^0.19.9",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mailhog": "^4.16.0",
    "plop": "^4.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.2",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.7",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  }
}
