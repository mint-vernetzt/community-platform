    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^15.1.3",
    "react-image-crop": "^11.0.1",
    "react-quill": "^2.0.0",
    "remix-forms": "^2.2.1",
