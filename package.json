    "prisma:migrate": "node -r dotenv/config node_modules/.bin/prisma migrate dev",
    "prisma:format": "npx prisma format",
    "copy:fonts": "rsync node_modules/@fontsource/source-sans-pro/files/source-sans-pro-latin-{400,600,700,900}-normal.woff public/fonts",
